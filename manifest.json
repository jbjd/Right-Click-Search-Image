{
    "manifest_version": 3,
    "name": "Image Searcher",
    "description": "Chrome extension that adds a right click option to images, using google lens to search for them",
    "version": "0.1.1",
    "author": "James Demetris",
    "icons": {
        "128": "/images/icon128.png"
    },
    "action": {
        "default_icon": {
            "128": "/images/icon128.png"
        }
    },
    "background": {
        "service_worker": "script.js"
    },
    "permissions": [
        "contextMenus",
        "tabs"
    ]
}